user_input = input("Enter a number between 0 and 9: ")
if user_input.isdigit() and 0 <= int(user_input) <= 9:
    num = int(user_input)
    if num == 0:
        print("You entered zero!")
    elif num == 1:
        print("You entered one!")
    elif num == 2:
        print("You entered two!")
    elif num == 3:
        print("You entered three!")
    elif num == 4:
        print("You entered four!")
    elif num == 5:
        print("You entered five!")
    elif num == 6:
        print("You entered six!")
    elif num == 7:
        print("You entered seven!")
    elif num == 8:
        print("You entered eight!")
    elif num == 9:
        print("You entered nine!")
else:
    print("You did not enter a number between 0 and 9.")
